{
  "name": "@hexlet/analytics-plugin-yandex-metrika",
  "version": "1.0.5",
  "type": "module",
  "description": "Yandex Metrika plugin for DavidWells Analytics",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build",
    "lint": "tsc --noEmit",
    "release": "release-it"
  },
  "peerDependencies": {
    "analytics": "^0.8.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "jiti": "^2.4.2",
    "npm-check-updates": "^18.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "dependencies": {
    "release-it": "^19.0.2"
  },
  "release-it": {
    "hooks": {
      "before:release": "npm run build"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "conventionalcommits",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hexlet/analytics-plugin-yandex-metrika.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "analytics",
    "Yandex Metrika"
  ],
  "author": "Hexlet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hexlet/analytics-plugin-yandex-metrika/issues"
  },
  "homepage": "https://github.com/Hexlet/analytics-plugin-yandex-metrika#readme"
